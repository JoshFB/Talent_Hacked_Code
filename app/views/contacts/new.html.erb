<%= form_for [@client, @contact] do |form| %>
	<%= form.label :name %>
	<%= form.text_field :name %>

	<%= form.label :email %>
	<%= form.text_field :email %>

	<%= form.label :phone %>
	<%= form.text_field :phone %>

	<%= form.label :website %>
	<%= form.text_field :website %>

	<%= form.submit %>
<% end %>