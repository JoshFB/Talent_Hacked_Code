
<h2 class="page-title">Account Details</h2>



<%= form_for(current_developer, as: :developers, url: developers_profile_path(current_developer), html: { method: :put }) do |f| %>

	<%= f.label :name %>
	<%= f.text_field :name %>

	<%= f.label :email %>
	<%= f.text_field :email %>


<%= f.submit %>

<% end %>
