    <div class="navbar navbar-inverse navbar-fixed-top" role="navigation">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="#" alt='Talent Hacked'><img src="/assets/talent_hacked_italic_swoosh_white.png"></a>
        </div>
        <div class="navbar-collapse collapse">

      <% if current_developer %>  
        <%= link_to 'Dashboard', dashboard_path, method: :get, class: "navbar-text navbar-right" %>
        <%= link_to 'Sign out', destroy_developer_session_path, method: :delete, class: "navbar-text navbar-right" %>
      <% elsif current_client %>  
        <%= link_to 'Dashboard', dashboard_path, method: :get, class: "navbar-text navbar-right" %>
        <%= link_to 'Sign out', destroy_client_session_path, method: :delete, class: "navbar-text navbar-right" %>
      <% else %>
        <%= link_to "Developer Sign in", new_developer_session_path, class: "navbar-text navbar-right" %> 
        <%= link_to "Client Sign in", new_client_session_path, class: "navbar-text navbar-right" %> 
        <%= link_to "Developer Sign in with LinkedIn", developer_omniauth_authorize_path(:linkedin), class: "navbar-text navbar-right" %>
      <% end %>

        </div><!--/.navbar-collapse -->
      </div>
    </div>

    <!-- Main jumbotron for a primary marketing message or call to action -->
    <div class="jumbotron">
      <% if developer_signed_in? %>
      <h1 align="center"><%= "Welcome #{current_developer.email}" %></h1>
      <% elsif client_signed_in? %>
      <h1 align="center"><%= "Welcome #{current_client.email}" %></h1>
      <% else %>
      <h1 align="center"><%= "Welcome to Talent Hacked" %></h1>
      <% end %>
      <% if !(current_developer || current_client) %>
        <div class="container">
          <p align="center">
          <%= link_to "Clients", new_client_registration_path, class: "btn btn-primary btn-lg" %>
          <%= link_to "Developers", new_developer_registration_path, class: "btn btn-primary btn-lg" %></p>
        </div>
      <% end %>
